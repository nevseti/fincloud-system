# Environment variables for production deployment
# Copy this file to .env and fill in the values

# Database
POSTGRES_PASSWORD=your_secure_password_here

# Security
SECRET_KEY=your_super_secret_key_for_jwt_tokens

# Service URLs (for production)
AUTH_SERVICE_URL=https://your-app.railway.app/auth
FINANCE_SERVICE_URL=https://your-app.railway.app/finance
REPORT_SERVICE_URL=https://your-app.railway.app/reports

# CORS settings
ALLOWED_ORIGINS=https://your-app.railway.app,https://your-frontend.railway.app

