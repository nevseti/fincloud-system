{
  "name": "fincloud-frontend",
  "version": "1.0.0",
  "description": "FinCloud Frontend Application",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "collectCoverageFrom": [
      "*.js",
      "!node_modules/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
